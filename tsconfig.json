{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "sourceMap": true,
        "outDir": "dist",
        "declaration": true,
        "declarationDir": "declarations",
        "strictNullChecks": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true
    },
    "include": [
        "src/**/*.ts"
    ]
}
